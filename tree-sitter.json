{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bruno",
      "camelcase": "Bruno",
      "title": "Bruno",
      "scope": "source.bruno",
      "file-types": [
        "bruno",
        "bru"
      ],
      "injection-regex": "^bruno$",
      "class-name": "TreeSitterBruno"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Bruno grammar for tree-sitter",
    "authors": [
      {
        "name": "Scalamando"
      }
    ],
    "links": {
      "repository": "https://github.com/jesses-code-adventures/tree-sitter-bruno"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}